{
    "name": "pari-gp-lang",
    "displayName": "PARI/GP",
    "description": "Syntax highlighting for PARI/GP",
    "version": "0.0.1",
    "engines": {
        "vscode": "^1.53.0"
    },
    "categories": [
        "Programming Languages"
    ],
    "contributes": {
        "languages": [{
            "id": "gp",
            "aliases": ["PARI/GP", "gp"],
            "extensions": [".gp"],
            "configuration": "./language-configuration.json"
        }],
        "grammars": [{
            "language": "gp",
            "scopeName": "source.gp",
            "path": "./syntaxes/gp.tmLanguage.json"
        }]
    }
}